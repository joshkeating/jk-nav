{
  "manifest_version": 2,

  "name": "jk-nav",
  "description": "This extension allows the user to navigate defined webpages using keyboard shortcuts.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["<put js to inject here>"],
      "run_at": "document_start"
    }
  ],

  "options_page":"options.html",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Change nav behavior for this site"
  }

}
